<?xml version='1.0' encoding='utf-8'?>
<widget id="com.wistron.wsh.wibox" version="0.0.9" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>WiBox</name>
    <description>
        A sample Cordova application that simulate WiBox client.
    </description>
    <author email="alex_huang@wistron.com" href="http://www.wistron.com">
        Alex Huang
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="www/img/wiboxlogo.png" />
</widget>
